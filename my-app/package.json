{
  "name": "tracker-hunt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service build --mode development --watch",
    "build": "vue-cli-service build",
    "test:unit": "vitest run --environment jsdom",
    "test:coverage": "vitest run --environment jsdom --coverage",
    "lint": "vue-cli-service lint",
    "start": "puppeteer",
    "test": "jest",
    "e2e": "jest --detectOpenHandles"
  },
  "dependencies": {
    "@auth0/auth0-vue": "^1.0.2",
    "axios": "^0.27.2",
    "bootstrap-vue": "^2.22.0",
    "chart.js": "^3.9.1",
    "chrome-extensions-id": "^1.0.0",
    "core-js": "^3.8.3",
    "font-awesome": "^4.7.0",
    "http2": "^3.3.7",
    "json": "^11.0.0",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "requests": "^0.3.0",
    "sass": "^1.54.4",
    "snowpack": "^3.8.8",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "util": "^0.12.4",
    "vee-validate": "^4.5.11",
    "vue": "^3.2.13",
    "vue-3-socket.io": "^1.0.5",
    "vue-chartjs": "^4.1.1",
    "vue-socket.io": "^3.0.10",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/preset-env": "^7.18.10",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.5",
    "c8": "^7.12.0",
    "cypress": "^10.4.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.5.1",
    "jest-puppeteer": "^6.1.1",
    "node-sass": "^7.0.1",
    "puppeteer": "^13.0.1",
    "sass-loader": "^13.0.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "vitest": "^0.21.1",
    "vue-cli-plugin-browser-extension": "npm:@rhilip/vue-cli-plugin-browser-extension@^0.26.0",
    "webpack": "^5.74.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "webextensions": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      },
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      },
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
